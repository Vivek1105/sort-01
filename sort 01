def sort01(arr,n):
    i = 0
    j = n-1
    while i<j:
        if arr[i] == 0:
            i = i+1
            
           else:
               if arr[j] == 1 :
                    j = j-1
                    
                else:
                    arr[i] ,arr[j] = arr[j], arr[i]
                    j = j-1
                    
                   
